{% if fields.body %}
     <{{ fields.body.wrapper_element }}{{ fields.body.wrapper_attributes }}>
          {{ fields.body.content }}
     </{{ fields.body.wrapper_element }}>
{% endif %}

<div class="ancestor-demands">
     <{{ fields.field_ancestor_demands.wrapper_element }}{{ fields.field_ancestor_demands.wrapper_attributes }}>
          <span class="label">Demands</span><span class="inline-field-value">{{ fields.field_ancestor_demands.content }}</span>
     </{{ fields.field_ancestor_demands.wrapper_element }}>
</div>