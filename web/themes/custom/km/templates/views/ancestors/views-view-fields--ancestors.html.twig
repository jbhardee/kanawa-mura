<{{ fields.title.wrapper_element }}{{ fields.title.wrapper_attributes }}>
     <{{ fields.title.element_type }}{{ fields.title.element_attributes }}>
          {% if row.search_result %}
               <div class="search-result-row-header">     
                    <div class="result-title">
                         <a href="{{ row.link }}" title="{{ fields.title.content }}">{{ fields.title.content }}</a> ({{ fields.point_value_numeric.content }} Points){% if fields.source %} [{{ fields.source.content }}]{% endif %}
                    </div>

                    <div class="result-type">
                         <span class="result-icon"><i class="fa-solid fa-ghost"></i></span>{{ fields.clan_term.content }} Ancestor
                    </div>
          {% else %}
               <a id="{{ fields.title.content }}"></a>{{ fields.title.content }}{% if row.minorClan %}[ <a href="/families#{{ row.minorClan }}" class="anchor">{{ row.minorClan }}</a>]{% endif %} ({{ fields.point_value_numeric.content }} Points){% if fields.source %} [{{ fields.source.content }}]{% endif %}
          {% endif %}
     </{{ fields.title.element_type }}>
</{{ fields.title.wrapper_element }}>

